public class TollBooth extends Vechile implements Toll{

	 public TollBooth(int axels, float weight, String truckName) {
		super(axels, weight, truckName);
		
	}
	int sum;
	@Override
	public void toll() {
		sum=(int) (5*getAxels()+(getWeight()/500));
		System.out.println("Total Calculated toll for veichle:"+sum);
		
	}
	

}
