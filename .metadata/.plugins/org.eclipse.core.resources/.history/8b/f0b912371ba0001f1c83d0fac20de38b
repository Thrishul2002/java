public class Vechile implements Truck{
	private int axels;
	private int weight;
	private String truckName;
	
	public Vechile(int axels, int weight, String truckName) {
		this.axels = axels;
		this.weight = weight;
		this.truckName = truckName;
	}

	public int getAxels() {
		return axels;
	}

	public int getWeight() {
		return weight;
	}

	public String getTruckName() {
		return truckName;
	}

	@Override
	public void truck() {
		
		System.out.println(getAxels()+getWeight()+getTruckName());
		
	}

	

	
		
}
