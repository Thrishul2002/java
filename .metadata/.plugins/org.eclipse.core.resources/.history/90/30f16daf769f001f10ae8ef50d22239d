public class Vechile implements Truck{
	private int axels;
	private float weight;
	private String truckName;
	
	public Vechile(int axels, float weight, String truckName) {
		this.axels = axels;
		this.weight = weight;
		this.truckName = truckName;
	}

	public int getAxels() {
		return axels;
	}

	public float getWeight() {
		return weight;
	}

	public String getTruckName() {
		return truckName;
	}

	@Override
	public void truck() {
		System.out.println("Truck name: "+getTruckName()+" Axels: "+getAxels()+" weight of Truck: "+getWeight());
		
	}
		
}
