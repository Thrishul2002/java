public class TestTollBooth extends TollBooth {

	public TestTollBooth(int axels, float weight, String truckName) {
		super(axels, weight, truckName);
		// TODO Auto-generated constructor stub
	}
	public static void collections() {
		int truckCnt=0;
		if(getTruckName()!=null)
			truckCnt++;
		System.out.println(truckCnt);
	}
	public static void main(String[] args) {
		TestTollBooth tb=new TestTollBooth(5, 12000, "tata");
		TestTollBooth tb1=new TestTollBooth(4, 19000, "mahindra");
		tb.truck();
		tb.toll();
		collections();
	}

}
